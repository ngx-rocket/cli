{
  "name": "@ngx-rocket/cli",
  "version": "1.5.0",
  "description": "CLI for creating Angular apps with ngX-Rocket generators",
  "repository": "ngx-rocket/cli",
  "main": "index.js",
  "bin": {
    "ngx": "./bin/ngx"
  },
  "scripts": {
    "lint": "eslint *.js",
    "test": "npm run lint -s && ./test/test.sh",
    "postpublish": "git tag -a $npm_package_version -m '$npm_package_version' && git push --tags"
  },
  "keywords": [
    "ngx",
    "rocket",
    "angular",
    "ionic",
    "cli",
    "enterprise",
    "starter",
    "kit",
    "generator",
    "tool",
    "yeoman",
    "add-on",
    "awesome"
  ],
  "author": "Yohan Lasorsa",
  "license": "MIT",
  "homepage": "https://github.com/ngx-rocket/cli",
  "bugs": {
    "url": "https://github.com/ngx-rocket/cli/issues"
  },
  "dependencies": {
    "@ngx-rocket/ascii-logo": "^1.0.1",
    "chalk": "^2.0.1",
    "conf": "^1.3.1",
    "figures": "^2.0.0",
    "generator-ngx-rocket": "^2.2.1",
    "generator-ngx-rocket-addon": "^1.2.0",
    "inquirer": "^3.3.0",
    "minimist": "^1.2.0",
    "update-notifier": "^2.3.0",
    "yeoman-environment": "^2.0.4"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-xo-space": "^0.16.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintConfig": {
    "extends": "xo-space/esnext"
  }
}
